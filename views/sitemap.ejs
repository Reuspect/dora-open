<!-- BEGIN CONTAINER -->
<style>
    .siteMap{padding: 15px;}
    .siteMap ol li{height: 25px;}
</style>
<div class="container min-hight" style="margin-bottom: 20px;background: #FFFFFF">

    <div class="col-md-12 siteMap">
        <p>更新时间：<%=updateTime%></p>
        <ol>
        <%details.forEach(function(item){%>
            <li><a href="http://<%=myDomain%>/details/<%=item._id%>.html"><%=item.title%></a></li>
        <%})%>
        </ol>
    </div>

</div>
<!-- END CONTAINER -->